<odoo>
  <template id="general_ledger_custom_main_template">
    <t t-call="web.html_container">
      <t t-set="company" t-value="res.company"/>
      <div class="header">
        <h2>General Ledger</h2>
        <p>
          <strong>Period:</strong>
          <span t-esc="date_from"/> to <span t-esc="date_to"/>
        </p>
      </div>
      <div class="footer">
        <p>Generated by Odoo</p>
      </div>
      <div class="body">
        <t t-raw="lines"/>
      </div>
    </t>
  </template>
</odoo>
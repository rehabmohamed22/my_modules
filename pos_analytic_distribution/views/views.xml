<odoo>
    <data>
        <record id="res_config_settings_view_form_analytic" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.point_of_sale</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
            <field name="arch" type="xml">
                <block id="restaurant_section" position="after">
                    <block title="Analytic Distribution" id="analytic_section">
                        <setting>
                            <field name="pos_analytic_account_id"/>
                        </setting>
                    </block>
                </block>
            </field>
        </record>
    </data>
</odoo>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_vehicle_purchase" model="ir.sequence">
            <field name="name">Vehicle Purchase</field>
            <field name="code">vehicle.purchase.seq</field>
            <field name="prefix">PR/</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_vehicle_purchase_quotation" model="ir.sequence">
            <field name="name">Vehicle Purchase RFQ</field>
            <field name="code">vehicle.purchase.quotation.seq</field>
            <field name="prefix">RFQ/</field>
            <field name="padding">5</field>
        </record>
        <record id="seq_vehicle_purchase_order" model="ir.sequence">
            <field name="name">Vehicle Purchase PO</field>
            <field name="code">vehicle.purchase.order.seq</field>
            <field name="prefix">PO/</field>
            <field name="padding">5</field>
        </record>

    </data>
</odoo>
